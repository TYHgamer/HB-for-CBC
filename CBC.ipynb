    "  int<lower=1> N;//Total number of choice sets observed\n",
    "  int<lower=1,upper=R> r[N];//Respondent ID\n",
    "  int<lower=1,upper=S> y[N];//alternative selected\n",
    "  row_vector[K] x1[N];//variables with correlated coefs for choice 1\n",
    "  row_vector[K] x2[N];//variables with correlated coefs for choice 2\n",
    "  row_vector[K] x3[N];//variables with correlated coefs for choice 3\n",
    "  }\n",
    "  \n",
    "  parameters{\n",
    "  vector[K] mu;//prior for mean coefs\n",
    "  vector[K] beta[R];//correlated coefficients for each respondent\n",
    "  corr_matrix[K] Omega;//correlation matrix\n",
    "  vector<lower=0,upper=100>[K] tau;//variances of correlated coefficients\n",
    "  }\n",
    "  \n",
    "  \n",
    "  model{\n",
    "  matrix[K,K] Sigma;\n",
    "  Sigma = quad_form_diag(Omega,tau);//covariance matrix\n",
    "  tau~cauchy(0,2.5);//alternative prior for tau\n",
    "  Omega~lkj_corr(1);//Prior for Omega (correlation matrix)\n",
    "  mu~normal(0,100);\n",
    "  \n",
    "  \n",
    "  for (i in 1:R){ // Respondent-specific draws\n",
    "    beta[i]~multi_normal(mu,Sigma);//model for individual beta coefs\n",
    "  }\n",
    "  \n",
    "  for (n in 1:N){\n",
    "    vector[3] p;//choice probabilities\n",
    "    p[1] = exp(x1[n]*beta[r[n]]);\n",
    "    p[2] = exp(x2[n]*beta[r[n]]);\n",
    "    p[3] = exp(x3[n]*beta[r[n]]);\n",
    "    p = p/sum(p);\n",
    "    y[n]~categorical(p);//model for choice\n",
    "  }\n",
    "}\n",
    "\"\"\""
   ]
