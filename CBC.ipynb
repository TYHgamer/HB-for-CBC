   "source": [
    "K = int(data_vhb.shape[1]) #自变量个数\n",
    "R = int(len(set(data['id']))) #受访者数量\n",
    "C = len(set(data['alt_id'])) #备选项数量\n",
    "N = int(x1.shape[0]) #每一个选项面临的选择集总数\n",
    "S = int(N/R) #每一个受访者面临的选择集总数\n",
    "\n",
    "y = list(data[data['is_chosen']==int(1)]['alt_id']) #每一个选择集被选择的备选项\n",
