    "def Anala(data,N,n,var,var_rp,newvar):\n",
    "    #属性层次参考点分解\n",
    "    #收益列\n",
    "    for i in range(0,N):\n",
    "        if any(bayes_result[bayes_result['parameters'].str.startswith(f'beta.{i+1}.{n}')]['summary.mean'] >= 0):\n",
    "            data.loc[(data[f'{var}'] >= data[f'{var_rp}'])\n",
    "                     &(data['id']==i+1),\n",
    "                     f'{newvar}_gain'] = abs(data[f'{var}'] - data[f'{var_rp}'])\n",
    "        elif any(bayes_result[bayes_result['parameters'].str.startswith(f'beta.{i+1}.{n}')]['summary.mean'] < 0):\n",
    "            data.loc[(data[f'{var}'] < data[f'{var_rp}'])\n",
    "                     &(data['id']==i+1),\n",
    "                     f'{newvar}_gain'] = abs(data[f'{var}'] - data[f'{var_rp}'])\n",
    "    data[f'{newvar}_gain'] = data[f'{newvar}_gain'].fillna(0)\n",
    "    data[f'{newvar}_gain'] = data[f'{newvar}_gain'].astype(int)\n",
    "\n",
    "    #损失列\n",
    "    for j in range(0,N):\n",
    "        if any(bayes_result[bayes_result['parameters'].str.startswith(f'beta.{j+1}.{n}')]['summary.mean'] >= 0):\n",
    "            data.loc[(data[f'{var}'] < data[f'{var_rp}'])&(data['id']==j+1),\n",
    "                     f'{newvar}_loss'] = abs(data[f'{var}'] - data[f'{var_rp}'])\n",
    "        elif any(bayes_result[bayes_result['parameters'].str.startswith(f'beta.{j+1}.{n}')]['summary.mean'] < 0):\n",
    "            data.loc[(data[f'{var}'] >= data[f'{var_rp}'])\n",
    "                     &(data['id']==j+1),\n",
    "                     f'{newvar}_loss'] = abs(data[f'{var}'] - data[f'{var_rp}'])\n",
    "    data[f'{newvar}_loss'] = data[f'{newvar}_loss'].fillna(0)\n",
    "    data[f'{newvar}_loss'] = data[f'{newvar}_loss'].astype(int)"
   ]
  },
